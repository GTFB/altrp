{
    "name": "jambo-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "engines": {
        "bun": ">=1.1.0"
    },
    "scripts": {
        "dev:site": "bun run --filter=jambo-site dev",
        "build:site": "bun run --filter=jambo-site build",
        "start:site": "bun run --filter=jambo-site start",
        "lint:site": "bun run --filter=jambo-site lint"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1"
        },
    "dependencies": {
        "marked": "^16.2.1"
    }
}